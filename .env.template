# GridBridge UK Environment Configuration
# Copy this to .env and fill in values

# =============================================================================
# Ingestion Parameters
# =============================================================================
GRIDBRIDGE_START_DATE=2025-01-15
GRIDBRIDGE_DAYS=1

# =============================================================================
# API Keys (optional - enables extended access)
# =============================================================================

# Elexon API key - register at https://www.elexon.co.uk/
ELEXON_API_KEY=

# Met Office DataPoint - register at https://www.metoffice.gov.uk/services/data/datapoint
METOFFICE_API_KEY=

# =============================================================================
# Docker Registry (for CI/CD)
# =============================================================================
DOCKER_REGISTRY=

# =============================================================================
# Development Settings
# =============================================================================

# Enable debug logging
DEBUG=false

# Skip PyPSA network construction (faster iteration)
SKIP_PYPSA=false
